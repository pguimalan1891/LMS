
@{
    ViewBag.Title = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Scripts
{
    @Helper.LoadCustomJS("ViewsJS/Accounting/JS_Accounting_RFP.js", Url)
}

<div class="row">
    <div class="col-md-12">
        <div class="page-header"><h1>Accounting System <small>Request for Payment</small></h1></div>
    </div>
</div>
<div class="row">
    <div class="col-md-7">
        <form class="form-inline">
            <div class="form-group">
                <label for="selfindRFP">Find RFP:</label>
                <select class="form-control" id="selfindRFP">
                    <option value="10">For Approval</option>
                    <option value="3">Approved List</option>
                    <option value="2">Disapproved List</option>
                    <option value="1">Cancelled</option>
                    <option value="99">All Listing</option>
                </select>
            </div>
        </form>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <br />
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 id="lblfindRFP" class="panel-title">For Approval</h3>
            </div>
            <div class="panel-body">
                <div class="table-responsive">
                    <table id="tbAccountingRFP" class="table table-striped table-bordered dt-responsive no-wrap" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                <th>Status</th>
                                <th>RFP No</th>
                                <th>Date</th>
                                <th>Branch</th>
                                <th>Expense</th>
                                <th>Amount</th>
                                <th>Prepared By</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
