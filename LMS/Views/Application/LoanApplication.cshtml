
@model BusinessObjects.LoanApplicationModel
@{
    ViewBag.Title = "LoanApplication";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Scripts
{
    @Helper.LoadCustomJS("ViewsJS/Application/JS_ApplicationCtrl.js", Url)
}
<script>
    var loanModel = @Html.Raw(Json.Encode(Model));

</script>

<div class="panel panel-info">
    <div class="panel panel-heading">
        <h2>Loan Application</h2>
    </div>
    <div class="panel panel-body">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-7">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>Borrower Profile</h5>
                        </div>
                        <div class="ibox-content">
                            <div class="form-horizontal">
                                 <div class="form-group">
                                    <label class="col-lg-2 control-label">Borrower Code:</label>
                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" id="fld_la_BorrowerCode" value="@Model.borrowerProfile.CODE" readonly="readonly"></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Full Name:</label>
                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" value="@Model.borrowerProfile.Fullname" readonly="readonly"></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Landline:</label>
                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" value="@Model.borrowerProfile.LandLine" readonly="readonly"></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Mobile No.:</label>
                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" value="@Model.borrowerProfile.MobileNumber" readonly="readonly"></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Province/ City:</label>
                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" value="@Model.borrowerProfile.ProvinceCity" readonly="readonly"></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Barangay:</label>
                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" value="@Model.borrowerProfile.Barangay" readonly="readonly"></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Street Address:</label>
                                    <div class="col-lg-5"><input type="text" placeholder="" class="form-control" value="@Model.borrowerProfile.STREET_ADDRESS" readonly="readonly"></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Postal Code:</label>
                                    <div class="col-lg-5"><input type="text" placeholder="" class="form-control"  value="@Model.borrowerProfile.POSTAL_CODE"  readonly="readonly"></div>

                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                    <div class="col-md-5">
                        <div class="ibox float-e-margins">
                            <div class="ibox-title">
                                <h5>General Information</h5>
                            </div>
                            <div class="ibox-content">
                                <div class="form-horizontal">
                                    <div class="form-group">
                                        <label class="col-lg-2 control-label">Account No.:</label>
                                        <div class="col-lg-10"><input type="text" placeholder="" class="form-control" id="AccountNo" value="@Model.AccountNo" readonly="readonly"></div>

                                    </div>
                                    <div class="form-group">
                                        <label class="col-lg-2 control-label">Transaction Date Date.:</label>

                                        <div class="col-lg-10"><input type="text" placeholder="" class="form-control" id="fld_la_trandate" value="@Model.TransactionDate.ToShortDateString()"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        

                    </div>

               

            </div>

                <div class="row">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>Loan Information</h5>
                        </div>
                        <div class="ibox-content">
                            <div class="form-horizontal">
                                <p>Please fill-in all required fields.</p>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Purpose of Loan:</label>

                                    <div class="col-lg-10">
                                       <textarea cols="50" rows="5" id="fld_la_LoanPurpose">@Model.LoanPurpose</textarea>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">District/Branch:</label>

                                    <div class="col-lg-5">
                                        <select class="form-control" id="fld_la_District">
                                            @foreach (var district in Model.districts)
                                            {
                                                // cut for brevity, need to add back more code from original
                                                if (Model.DistrictCode != null && Model.DistrictCode == district.Code)
                                                {
                                                    <option value="@district.DistrictID" selected="selected">
                                                        @district.Description
                                                    </option>
                                                }
                                                else
                                                {
                                                    <option value="@district.DistrictID">
                                                        @district.Description
                                                    </option>
                                                }

                                            }
                                           
                                            
                                        </select>
                                    </div>
                                    <div class="col-lg-5">
                                        <select class="form-control" id="fld_la_Branch">
                                            @foreach (var org in Model.orgs)
                                            {
                                                // cut for brevity, need to add back more code from original


                                                if (Model.DistrictCode != null && Model.DistrictCode == org.DistrictID)
                                                {
                                                    var distId = "";
                                                    foreach (var district in Model.districts)
                                                    {
                                                        if (Model.DistrictCode == district.Code)
                                                        {
                                                            distId = district.DistrictID;
                                                        }
                                                    }
                                                    if (distId != "")
                                                    {
                                                        if (distId == org.DistrictID)
                                                        {
                                                            <option value="@org.OrganizationID" selected="selected">
                                                                @org.Description
                                                            </option>
                                                        }
                                                        else
                                                        {
                                                            <option value="@org.OrganizationID">
                                                                @org.Description
                                                            </option>
                                                        }

                                                    }
                                                    else
                                                    {
                                                        <option value="@org.OrganizationID">
                                                            @org.Description
                                                        </option>
                                                    }

                                                }
                                                else
                                                {
                                                    <option value="@org.OrganizationID">
                                                        @org.Description
                                                    </option>
                                                }

                                            }
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Application Type:</label>

                                    <div class="col-lg-5">
                                        <select class="form-control" id="fld_la_AppType">
                                            @foreach (var appType in Model.applicationTypes)
                                            {
                                                if (Model.ApplicationType != null && Model.ApplicationType == appType.ApplicationTypeID)
                                                {
                                                    // cut for brevity, need to add back more code from original
                                                    <option value="@appType.ApplicationTypeID" selected="selected">
                                                        @appType.Description
                                                    </option>
                                                }
                                                else
                                                {
                                                    // cut for brevity, need to add back more code from original
                                                    <option value="@appType.ApplicationTypeID">
                                                        @appType.Description
                                                    </option>
                                                }

                                            }
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Agent:</label>

                                    <div class="col-lg-5">
                                        <select class="form-control" id="fld_la_Agent">
                                            <option>Branch One</option>
                                            <option>Branch One</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Product:</label>

                                    <div class="col-lg-5">
                                        <select class="form-control" id="fld_la_Product">
                                            @foreach (var product in Model.products)
                                            {
                                                // cut for brevity, need to add back more code from original
                                                if (Model.ProductId != null && Model.ProductId == product.ID)
                                                {
                                                    // cut for brevity, need to add back more code from original
                                                    <option value="@product.ID" selected="selected">
                                                        @product.DESCRIPTION
                                                    </option>
                                                }
                                                else
                                                {
                                                    // cut for brevity, need to add back more code from original
                                                    <option value="@product.ID">
                                                        @product.DESCRIPTION
                                                    </option>
                                                }
                                              
                                            }
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Set:</label>

                                    <div class="col-lg-5">
                                        <select class="form-control" id="fld_la_LoanSet">
                                            @foreach (var set in Model.sets)
                                            {
                                                // cut for brevity, need to add back more code from original
                                               

                                                if (Model.SetId != null && Model.SetId == set.ID)
                                                {
                                                    // cut for brevity, need to add back more code from original
                                                    <option value="@set.ID" selected="selected">
                                                        @set.DESCRIPTION
                                                    </option>
                                                }
                                                else
                                                {
                                                    // cut for brevity, need to add back more code from original
                                                    <option value="@set.ID">
                                                        @set.DESCRIPTION
                                                    </option>
                                                }
                                            }
                                        </select>
                                    </div>
                                </div>

        
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Terms</label>

                                    <div class="col-lg-5">
                                        <select class="form-control" id="fld_la_LoanTerms">
                                            @foreach (var term in Model.terms)
                                            {
                                                // cut for brevity, need to add back more code from original
                                                if (Model.TermsId != null && Model.TermsId == term.ID)
                                                {
                                                    // cut for brevity, need to add back more code from original
                                                    <option value="@term.ID" selected="selected">
                                                        @term.DESCRIPTION
                                                    </option>
                                                }
                                                else
                                                {
                                                    // cut for brevity, need to add back more code from original
                                                    <option value="@term.ID">
                                                        @term.DESCRIPTION
                                                    </option>
                                                }
                                              
                                            }
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label" id="fld_la_Factor">Factor Rate:</label>

                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" value="@Model.FactorRate"></div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Desired MLV:</label>

                                    <div class="col-lg-10"><input type="text" placeholder="" id="fld_la_DesiredMLV" class="form-control" value="@Model.DesiredMLV"></div>
                                </div>


                            </div>
                        </div>
                    </div>

                </div>
            <div class="row">
                <div class="col-md-9">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>Co-Maker/s</h5><div class="btn btn-success pull-right btn-sm" data-toggle="modal" data-target="#comakerModal"  style="margin-top:-5px;"><span class="fa fa-plus"></span></div>
                        </div>
                        <div class="ibox-content">
                            <div class="form-horizontal" id="">
                                <table id="tblComakers" class="table table-striped table-bordered dt-responsive no-wrap" cellspacing="0" width="100%">
                                    <thead>
                                        <tr>
                                            <th>First Name</th>
                                            <th>Middle Name</th>
                                            <th>Last Name</th>
                                            <th>Date of Birth</th>
                                            <th>Phone No.</th>
                                            <th>Address</th>
                                          

                                        </tr>

                                    </thead>
                                </table>
                            </div>
                        </div>
                    </div>


                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>List of Collaterals</h5>
                            <div class="btn btn-success pull-right btn-sm"  data-toggle="modal" data-target="#collateralModal"  style="margin-top:-5px;"><span class="fa fa-plus"></span></div>
                        </div>
                        <div class="ibox-content">
                            <div class="form-horizontal" id="collateral-list">
                                <table id="tblCollateral" class="table table-striped table-bordered dt-responsive no-wrap" cellspacing="0" width="100%">
                                    <thead>
                                        <tr>
                                            <th>Max Loan Value</th>
                                            <th>Appraised Value</th>
                                            <th>Loan Value</th>
                                            <th>Collateral Group</th>
                                            <th>Collateral Type</th>
                                            <th>Description</th>
                                            <th>Usage</th>
                                            <th>OR Expiry Date</th>
                                            <th>Additional Info</th>

                                        </tr>

                                    </thead>
                                </table>
                            </div>
                        </div>
                    </div>


                </div>
            </div>
            <div class="row">
                <div class="col-md-7">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>Credit Investigation Info </h5>
                            <div class="btn btn-success pull-right btn-sm" data-toggle="modal" data-target="#collateralModal" style="margin-top:-5px;">View C.I. Report</div>
                        </div>
                        <div class="ibox-content">
                            <div class="form-horizontal">
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Recommended MLV:</label>
                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" value="" readonly="readonly"></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Original MLV:</label>
                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" value=" " id="fld_la_OriginalMLV" ></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Approved MLV:</label>
                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" value=" " id="fld_la_ApprovedMLV"></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Factor Rate:</label>
                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" value=" " id="fld_la_ciFactor"></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Recommended Rate/Nominal:</label>
                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" value=" " id="fld_la_RecRate" ></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Default Handling Fee:</label>
                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" value="Not yet available" readonly="readonly"></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Default PPD Amount:</label>
                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" value="Not yet available" readonly="readonly"></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Agent Incentive:</label>
                                    <div class="col-lg-5"><input type="text" placeholder="" class="form-control" value=" " id="fld_la_AgentIncent" ></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Dealer Incentive:</label>
                                    <div class="col-lg-5"><input type="text" placeholder="" class="form-control" value=" " id="fld_la_DealIncent" ></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Credit Investigator</label>
                                    <div class="col-lg-5"><input type="text" placeholder="" class="form-control" value=" " id="fld_la_CCI" readonly="readonly"></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Assured</label>
                                    <div class="col-lg-5"><input type="text" placeholder="" class="form-control" value=" " id="fld_la_Assured" ></div>

                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="col-md-5">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>Required Documents</h5>
                        </div>
                        <div class="ibox-content">
                            <div class="form-horizontal">
                                <ul>
                                    @foreach (var doc in Model.reqDocs)
                                    {
                                        // cut for brevity, need to add back more code from original
                                        <li>
                                            @doc.description
                                        </li>
                                    }
                                </ul>
                            </div>
                        </div>
                    </div>


                </div>

            </div>
            <div class="row">
                <div class="col-md-7">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>Notes</h5>
                        </div>
                        <div class="ibox-content">
                            <div class="form-horizontal">
                                <div class="form-group">
                                    <label class="col-lg-2 
                                           control-label">Notes:</label>
                                    <div class="col-lg-10"><textarea cols="50" rows="10" id="fld_la_Notes"></textarea></div>

                                </div>
                             </div>
                        </div>
                    </div>

                </div>
                <div class="col-md-5">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>Attached Documents</h5>
                        </div>
                        <div class="ibox-content">
                            <div class="form-horizontal">

                            </div>
                        </div>
                    </div>


                </div>

            </div>
            <div class="row">
                <div class="pull-right">
                    <div class="btn btn-success" onclick="insertLoan()">
                        Submit
                    </div>
                    <div class="btn btn-danger">
                        Cancel
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>
<div class="modal fade " tabindex="-1" role="dialog" id="collateralModal" >
    <div class="modal-dialog modal-lg" role="document" >
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Collateral Details</h4>
            </div>
            <div class="modal-body ">
                <div class="container-fluid">
                    <div class="form-group col-md-6">
                        <label class="col-md-3 control-label">Max Loan Value</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" id="fld_coll_MLV" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6">
                        <label class="col-md-3 control-label">Appraised Value</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" id="fld_coll_APPRAISED_VALUE" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6">
                        <label class="col-md-3 control-label">Loan Value</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" id="fld_coll_LOAN_VALUE" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6">
                        <label class="col-md-3 control-label">Collateral Group</label>
                        <div class="col-md-9">
                            <select class="form-control" id="colGroup" onchange="changeCollateralFields(this.value)">
                                <option value="1">
                                    Vehicles and Heavy Equipment
                                </option>
                                <option value="2">
                                    Appliances and Other Equipment
                                </option>
                                <option value="3">
                                    Real Estate and Others
                                </option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group col-md-6">
                        <label class="col-md-3 control-label">Collateral Type</label>
                        <div class="col-md-9">
                            <select class="form-control" id="list_coltype">
                               
                            </select>
                        </div>
                    </div>
                    <div class="form-group col-md-6">
                        <label class="col-md-3 control-label">Description</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" id="fld_coll_DESCRIPTION" placeholder="">
                        </div>
                    </div>

                    <div class="form-group col-md-6">
                        <label class="col-md-3 control-label">Usage</label>
                        <div class="col-md-9">
                            <select class="form-control" id="list_collateralUsage">
                               
                            </select>
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col3">
                        <label class="col-md-3 control-label">TCT No.</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" id="fld_coll_TCT_NUMBER" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col2">
                        <label class="col-md-3 control-label">Serial No.</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" id="fld_coll_SERIAL_NUMBER" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col2 col1">
                        <label class="col-md-3 control-label">Year</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" id="fld_coll_YEAR" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col2 col1">
                        <label class="col-md-3 control-label">Model</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" id="fld_coll_MODEL" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col2 col1">
                        <label class="col-md-3 control-label">Color</label>
                        <div class="col-md-9">
                            <select class="form-control" id="list_color">
                               
                            </select>
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col1">
                        <label class="col-md-3 control-label">Fuel</label>
                        <div class="col-md-9">
                            <select class="form-control" id="list_fuelType">
                               
                            </select>
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col1">
                        <label class="col-md-3 control-label">Chassis No.</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" id="fld_coll_CHASSIS_NUMBER" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col1">
                        <label class="col-md-3 control-label">Engine No.</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" id="fld_coll_ENGINE_NUMBER" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col1">
                        <label class="col-md-3 control-label">Plate No.</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" id="fld_coll_PLATE_NUMBER" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col1">
                        <label class="col-md-3 control-label">ODO Reading</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" id="fld_coll_ODO_READING" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col1">
                        <label class="col-md-3 control-label">CRE No.</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" id="fld_coll_CRE_NUMBER" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col1">
                        <label class="col-md-3 control-label">CRE Name</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" id="fld_coll_CRE_NAME" placeholder="">
                        </div>
