
@model BusinessObjects.CreditInvestigation



 @{
    ViewBag.Title = "Credit Investigation Form";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Scripts
{
     @Helper.LoadCustomJS("ViewsJS/Application/JS_CreditInvCtrl.js", Url)
}
<script>
    var tmpCrObject = @Html.Raw(Json.Encode(Model));

</script>
<div class="panel panel-info">
    <div class="panel panel-heading">
        <h2>Credit Investigation</h2>
    </div>
    <div class="panel panel-body">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-7">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>General Information</h5>
                        </div>
                        <div class="ibox-content">
                            <div class="form-horizontal">
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">C.I. No.:</label>
                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" id="AccountNo" value="@Model.CINumber" readonly="readonly"></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Transaction Date.:</label>

                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" id="fld_la_trandate" value="@Model.Date"></div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Loan Application No.:</label>
                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" id="AccountNo" value="LA-@Model.CINumber" readonly="readonly"></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Customer No.:</label>
                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" id="AccountNo" value="@Model.borrowProf.Code" readonly="readonly"></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Full Name:</label>
                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" id="AccountNo" value="@Model.borrowProf.LastName, @Model.borrowProf.FirstName" readonly="readonly"></div>

                                </div>
                               
                                
                            </div>
                        </div>
                    </div>

                </div>
                <div class="col-md-5">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>Borrower Profile</h5>
                        </div>
                        <div class="ibox-content">
                            <div class="form-horizontal">
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Date of Birth</label>
                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" id="fld_la_BorrowerCode" value="@Model.borrowProf.DateofBirth" readonly="readonly"></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Gender</label>
                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" id="fld_la_BorrowerCode" value="@Model.borrowProf.Gender" readonly="readonly"></div>

                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Citizenship</label>
                                    <div class="col-lg-10"><input type="text" placeholder="" class="form-control" id="fld_la_BorrowerCode" value="Filipino" readonly="readonly"></div>

                                </div>
                            </div>
                        </div>
                    </div>


                </div>



            </div>
            <div class="row">
                <div class="col-md-7">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>Loan Information</h5>
                        </div>
                        <div class="ibox-content">
                            <div class="form-horizontal">
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Purpose of Loan:</label>

                                    <div class="col-lg-10">
                                        <textarea cols="50" rows="5" id="fld_la_LoanPurpose">@Model.loanModel.LoanPurpose</textarea>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Product:</label>

                                    <div class="col-lg-5">
                                        @foreach (var product in Model.loanModel.products)
                                        {
                                            // cut for brevity, need to add back more code from original
                                            if (Model.loanModel.ProductId != null && Model.loanModel.ProductId == product.ID)
                                            {
                                                // cut for brevity, need to add back more code from original
                                                <input value=" @product.DESCRIPTION" class="form-control"   type="text" readonly="readonly">

                                            }


                                        }  
                                         
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Term:</label>

                                    <div class="col-lg-5">
                                        
                                                <input value="@Model.TermsDescription" class="form-control"  type="text" readonly="readonly">

                                       
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Recommended MLV:</label>

                                    <div class="col-lg-5">
                                        <input value="@Model.loanModel.DesiredMLV" class="form-control"  type="text" id="DesiredMLV" >
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Monthly Installment:</label>

                                    <div class="col-lg-5">
                                        <input value="@Model.MonthlyInstallment" class="form-control"  type="text" readonly="readonly">
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>


                </div>
                <div class="col-md-5">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>Monthly Income Information</h5>
                        </div>
                        <div class="ibox-content">
                            <div class="form-horizontal">
                              
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Income:</label>

                                    <div class="col-lg-5">
                                        @foreach (var product in Model.incomes)
                                        {
                                            // cut for brevity, need to add back more code from original
                                            if (product.IncomeType == "EMPLOYED_INCOME")
                                            {
                                                // cut for brevity, need to add back more code from original
                                                <input value=" @product.IncomeAmount" type="text" class="form-control"  readonly="readonly" id="cir_income">

                                            }


                                        }
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Deduction:</label>

                                    <div class="col-lg-5">

                                        <input value="@Model.Deduction"  class="form-control"  type="text"  id="cir_deduction">

                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Net Income:</label>

                                    <div class="col-lg-5">

                                        <input value="@Model.NetIncome"  class="form-control" type="text" readonly="readonly" id="cir_netIncome">

                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Spouse Income:</label>

                                    <div class="col-lg-5">
                                        @foreach (var product in Model.incomes)
                                        {
                                            // cut for brevity, need to add back more code from original
                                            if (product.IncomeType == "SPOUSE_INCOME")
                                            {
                                                // cut for brevity, need to add back more code from original
                                                <input value=" @product.IncomeAmount"  class="form-control"  type="text" readonly="readonly"  id="cir_spouseIncome">

                                            }


                                        }
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Spouse Deduction:</label>

                                    <div class="col-lg-5">

                                        <input value="@Model.spouseDeduction"  class="form-control"  type="text" id="cir_spouseDeduction">

                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Spouse Net Income:</label>

                                    <div class="col-lg-5">

                                        <input value="@Model.spouseNetIncome"  class="form-control"  type="text" readonly="readonly"   id="cir_spouseNetIncome">

                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-2 control-label">Business Income:</label>

                                    <div class="col-lg-5">
                                        @foreach (var product in Model.incomes)
                                        {
                                            // cut for brevity, need to add back more code from original
                                            if (product.IncomeType == "BUSINESS_INCOME")
                                            {
                                                // cut for brevity, need to add back more code from original
                                                <input  class="form-control"  value=" @product.IncomeAmount" type="text" readonly="readonly"   id="cir_businessIncome">

                                            }


                                        }
                                    </div>
                                </div>
                              
                            </div>
                        </div>
                    </div>


                </div>

            </div>
            <div class="row">
                <div class="col-md-6">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>Monthly Expenses Information</h5>
                        </div>
                        <div class="ibox-content">
                            <div class="form-horizontal">

                                <div class="form-group">
                                    <label class="col-lg-4 control-label">Living Expenses:</label>

                                    <div class="col-lg-5"><input value="0" type="text"  class="form-control"  id="cir_livingExpense" onchange=" computeSummary()"></div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-4 control-label">Rentals:</label>

                                    <div class="col-lg-5"><input value="0" type="text" class="form-control"  id="cir_rentals" onchange=" computeSummary()"></div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-4 control-label">Light &amp; Water:</label>

                                    <div class="col-lg-5"><input value="0" type="text" class="form-control"  id="cir_lightWater" onchange=" computeSummary()"></div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-4 control-label">Education:</label>

                                    <div class="col-lg-5"><input value="0" type="text" class="form-control"  id="cir_education" onchange=" computeSummary()"></div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-4 control-label">Amortization:</label>

                                    <div class="col-lg-5"><input value="0" type="text" class="form-control"  id="cir_amortization" onchange=" computeSummary()"></div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-4 control-label">Transportation:</label>

                                    <div class="col-lg-5"><input value="0" type="text" class="form-control"  id="cir_transportation" onchange=" computeSummary()"></div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-4 control-label">Other Expenses:</label>

                                    <div class="col-lg-5"><input value="0" type="text" class="form-control"  id="cir_otherExpense" onchange=" computeSummary()"></div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-4 control-label">Total Expenses:</label>

                                    <div class="col-lg-5"><input value="0" type="text" class="form-control" readonly="true"  id="cir_totalExpenses"></div>
                                </div>
                               

                            </div>
                        </div>
                    </div>


                </div>
                <div class="col-md-6">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>Summary</h5>
                        </div>
                        <div class="ibox-content">
                            <div class="form-horizontal">

                                <div class="form-group">
                                    <label class="col-lg-4 control-label">Gross Disposable Income:</label>

                                    <div class="col-lg-5">
                                       
                                               <input value="0" type="text" class="form-control" readonly="readonly" id="cir_GDI">

                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-4 control-label">Class:</label>

                                    <div class="col-lg-5">

                                        <input value="0" type="text" class="form-control"  id="cir_Class">

                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-4 control-label">Net Disposable Income:</label>

                                    <div class="col-lg-5">

                                        <input value="0" type="text" class="form-control"  readonly="readonly" id="cir_NDI" />

                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-4 control-label">Monthly Installment:</label>

                                    <div class="col-lg-5">

                                        <input value="0" type="text"  class="form-control" id="cir_sumMonthlyInstallment">

                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-lg-4 control-label">Excess/Defeciency:</label>

                                    <div class="col-lg-5">

                                        <input value="0" type="text"  class="form-control" readonly="readonly" id="cir_Excess">

                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>


                </div>

            </div>

             <div class="row">
                <div class="col-md-9">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>Co-Maker/s</h5><div class="btn btn-success pull-right btn-sm" data-toggle="modal" data-target="#comakerModal" style="margin-top:-5px;"><span class="fa fa-plus"></span></div>
                        </div>
                        <div class="ibox-content">
                            <div class="form-horizontal" id="">
                                <table id="tblComakers" class="table table-striped table-bordered dt-responsive no-wrap" cellspacing="0" width="100%">
                                    <thead>
                                        <tr>
                                            <th>First Name</th>
                                            <th>Middle Name</th>
                                            <th>Last Name</th>
                                            <th>Date of Birth</th>
                                            <th>Phone No.</th>
                                            <th>Address</th>


                                        </tr>

                                    </thead>
                                </table>
                            </div>
                        </div>
                    </div>


                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>List of Collaterals</h5>
                            <div class="btn btn-success pull-right btn-sm" data-toggle="modal" data-target="#collateralModal" style="margin-top:-5px;"><span class="fa fa-plus"></span></div>
                        </div>
                        <div class="ibox-content">
                            <div class="form-horizontal" id="collateral-list">
                                <table id="tblCollateral" class="table table-striped table-bordered dt-responsive no-wrap" cellspacing="0" width="100%">
                                    <thead>
                                        <tr>
                                            <th>Max Loan Value</th>
                                            <th>Appraised Value</th>
                                            <th>Loan Value</th>
                                            <th>Collateral Group</th>
                                            <th>Collateral Type</th>
                                            <th>Description</th>
                                            <th>Usage</th>
                                            <th>OR Expiry Date</th>
                                            <th>Additional Info</th>

                                        </tr>

                                    </thead>
                                </table>
                            </div>
                        </div>
                    </div>


                </div>
            </div>
            <div class="row">
                <div class="col-md-7">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>Notes</h5>
                        </div>
                        <div class="ibox-content">
                            <div class="form-horizontal">
                                <div class="form-group">
                                    <label class="col-lg-2
                                           control-label">Notes:</label>
                                    <div class="col-lg-10"><textarea cols="50" rows="10" id="fld_la_Notes"></textarea></div>

                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="col-md-5">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>Attached Documents</h5>
                        </div>
                        <div class="ibox-content">
                            <div class="form-horizontal">

                            </div>
                        </div>
                    </div>


                </div>

            </div>
            <div class="row">
                <div class="pull-right">
                    <div class="btn btn-success" onclick="insertLoan()">
                        Submit
                    </div>
                    <div class="btn btn-danger">
                        Cancel
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>
<div class="modal fade " tabindex="-1" role="dialog" id="collateralModal">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Collateral Details</h4>
            </div>
            <div class="modal-body ">
                <div class="container-fluid">
                    <div class="form-group col-md-6">
                        <label class="col-md-3 control-label">Max Loan Value</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" readonly="readonly" id="fld_coll_MLV" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6">
                        <label class="col-md-3 control-label">Appraised Value</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" readonly="readonly" id="fld_coll_APPRAISED_VALUE" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6">
                        <label class="col-md-3 control-label">Loan Value</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" readonly="readonly" id="fld_coll_LOAN_VALUE" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6">
                        <label class="col-md-3 control-label">Collateral Group</label>
                        <div class="col-md-9">
                            <select class="form-control" readonly="readonly" id="colGroup" onchange="changeCollateralFields(this.value)">
                                <option value="1">
                                    Vehicles and Heavy Equipment
                                </option>
                                <option value="2">
                                    Appliances and Other Equipment
                                </option>
                                <option value="3">
                                    Real Estate and Others
                                </option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group col-md-6">
                        <label class="col-md-3 control-label">Collateral Type</label>
                        <div class="col-md-9">
                            <select class="form-control" readonly="readonly" id="list_coltype"></select>
                        </div>
                    </div>
                    <div class="form-group col-md-6">
                        <label class="col-md-3 control-label">Description</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" readonly="readonly" id="fld_coll_DESCRIPTION" placeholder="">
                        </div>
                    </div>

                    <div class="form-group col-md-6">
                        <label class="col-md-3 control-label">Usage</label>
                        <div class="col-md-9">
                            <select class="form-control" readonly="readonly" id="list_collateralUsage"></select>
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col3">
                        <label class="col-md-3 control-label">TCT No.</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" readonly="readonly" id="fld_coll_TCT_NUMBER" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col2">
                        <label class="col-md-3 control-label">Serial No.</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" readonly="readonly" id="fld_coll_SERIAL_NUMBER" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col2 col1">
                        <label class="col-md-3 control-label">Year</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" readonly="readonly" id="fld_coll_YEAR" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col2 col1">
                        <label class="col-md-3 control-label">Model</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" readonly="readonly" id="fld_coll_MODEL" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col2 col1">
                        <label class="col-md-3 control-label">Color</label>
                        <div class="col-md-9">
                            <select class="form-control" readonly="readonly" id="list_color"></select>
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col1">
                        <label class="col-md-3 control-label">Fuel</label>
                        <div class="col-md-9">
                            <select class="form-control" readonly="readonly" id="list_fuelType"></select>
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col1">
                        <label class="col-md-3 control-label">Chassis No.</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" readonly="readonly" id="fld_coll_CHASSIS_NUMBER" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col1">
                        <label class="col-md-3 control-label">Engine No.</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" readonly="readonly" id="fld_coll_ENGINE_NUMBER" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col1">
                        <label class="col-md-3 control-label">Plate No.</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" readonly="readonly" id="fld_coll_PLATE_NUMBER" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col1">
                        <label class="col-md-3 control-label">ODO Reading</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" readonly="readonly" id="fld_coll_ODO_READING" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col1">
                        <label class="col-md-3 control-label">CRE No.</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" readonly="readonly" id="fld_coll_CRE_NUMBER" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col1">
                        <label class="col-md-3 control-label">CRE Name</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" readonly="readonly" id="fld_coll_CRE_NAME" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col1">
                        <label class="col-md-3 control-label">CR Expiry Date</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" readonly="readonly" id="fld_coll_CR_EXPIRY_DATE" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col1">
                        <label class="col-md-3 control-label">OR No.</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" readonly="readonly" id="fld_coll_OR_NUMBER" placeholder="">
                        </div>
                    </div>

                    <div class="form-group col-md-6">
                        <label class="col-md-3 control-label">OR Expiry Date</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" readonly="readonly" id="fld_coll_OR_EXPIRATION_DATE" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col1">
                        <label class="col-md-3 control-label">Insurance Name</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" readonly="readonly" id="fld_coll_INSURANCE_NAME" placeholder="">
                        </div>
                    </div>
                    <div class="form-group col-md-6 collateralFld col1">
                        <label class="col-md-3 control-label">Insurance Expiry Date</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" readonly="readonly" id="fld_coll_INSURANCE_EXPIRATION_DATE" placeholder="">
                        </div>
                    </div>
                </div>
