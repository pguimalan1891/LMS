
@{
    ViewBag.Title = "ChangeCCI";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Scripts
{
    @Helper.LoadCustomJS("ViewsJS/Booking/JS_Booking_CCI.js", Url)
}

<div class="row">
    <div class="col-md-12">
        <div class="page-header"><h1>Workdesk <small>Change CCI Form</small></h1></div>
    </div>
</div>
<div class="row">
    <div class="col-md-7">
        <form class="form-inline">
            <div class="form-group">
                <label for="selfindCCI">Find CCI:</label>
                <select class="form-control" id="selfindCCI">
                    <option value="10">For Approval</option>
                    <option value="3">Approved List</option>
                    <option value="2">Disapproved List</option>
                    <option value="1">Cancelled</option>
                    <option value="99">All Listing</option>
                </select>
            </div>
        </form>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <br />
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 id="lblfindCCI" class="panel-title">For Approval</h3>
            </div>
            <div class="panel-body">
                <div class="table-responsive">
                    <table id="tbBookingCCI" class="table table-striped table-bordered dt-responsive no-wrap" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                <th>Status</th>
                                <th>Change CCI Form No</th>
                                <th>Date</th>
                                <th>Branch</th>
                                <th>Account No</th>
                                <th>Customer</th>
                                <th>Previous CCI</th>
                                <th>Current CCI</th>
                            </tr>

                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
