
@{
    ViewBag.Title = "CheckVoucher";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Scripts
{
    @Helper.LoadCustomJS("ViewsJS/Booking/JS_Booking_CV.js", Url)
}

<div class="row">
    <div class="col-md-12">
        <div class="page-header"><h1>Workdesk <small>Check Vouchers</small></h1></div>
    </div>
</div>
<div class="row">
    <div class="col-md-7">
        <form class="form-inline">
            <div class="form-group">
                <label for="selfindCV">Find CV:</label>
                <select class="form-control" id="selfindCV">
                    <option value="33">Not Ready</option>
                    <option value="11">For Endorsement</option>
                    <option value="47">For Funding</option>
                    <option value="55">Funded</option>
                    <option value="49">Released</option>
                    <option value="99">All Listing</option>
                </select>
            </div>
        </form>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <br />
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 id="lblfindCV" class="panel-title">Not Ready</h3>
            </div>
            <div class="panel-body">
                <div class="table-responsive">
                    <table id="tbBookingCV" class="table table-striped table-bordered dt-responsive no-wrap" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                <th>Status</th>
                                <th>CV No</th>
                                <th>Date</th>
                                <th>Branch</th>
                                <th>LMS DLR No</th>
                                <th>DLR No</th>
                                <th>Particulars</th>
                                <th>Account Type</th>
                                <th>Bank Name</th>
                                <th>Check No</th>
                                <th>Amount</th>
                                <th>FAO</th>
                                <th>Payee</th>
                                <th>Check Date</th>
                            </tr>

                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

