
@{
    ViewBag.Title = "CIRForm";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Scripts
{
    @Helper.LoadCustomJS("ViewsJS/Booking/JS_Booking_CIR.js", Url)
}

<div class="row">
    <div class="col-md-12">
        <div class="page-header"><h1>Workdesk <small>CIR Form</small></h1></div>
    </div>
</div>
<div class="row">
    <div class="col-md-7">
        <form class="form-inline">
            <div class="form-group">
                <label for="selfindCIR">Find CIR:</label>
                <select class="form-control" id="selfindCIR">
                    <option value="10">For Approval</option>
                    <option value="53">For Cash Card Deposit</option>
                    <option value="56">Forwarded to DV</option>
                    <option value="57">Forwarded to CV</option>
                    <option value="2">Disapproved List</option>
                    <option value="99">All Listing</option>
                </select>
            </div>
        </form>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <br />
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 id="lblfindCIR" class="panel-title">For Approval</h3>
            </div>
            <div class="panel-body">
                <div class="table-responsive">
                    <table id="tbBookingCIR" class="table table-striped table-bordered dt-responsive no-wrap" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                <th>Status</th>
                                <th>CIR Form No</th>
                                <th>Date</th>
                                <th>Branch</th>
                                <th>Account No</th>
                                <th>Loan Type</th>
                                <th>Loan Terms</th>
                                <th>LMS DLR No</th>
                                <th>DLR No</th>
                                <th>Agent</th>
                                <th>Commission</th>
                            </tr>

                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
